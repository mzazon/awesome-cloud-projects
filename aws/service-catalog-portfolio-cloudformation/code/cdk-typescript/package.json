{
  "name": "service-catalog-portfolio-cloudformation-cdk",
  "version": "1.0.0",
  "description": "CDK TypeScript implementation for Service Catalog Portfolio with CloudFormation Templates",
  "bin": {
    "service-catalog-portfolio": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "bootstrap": "cdk bootstrap",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\""
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "aws-cdk": "2.133.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.133.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "keywords": [
    "aws",
    "cdk",
    "service-catalog",
    "cloudformation",
    "infrastructure-as-code",
    "governance",
    "portfolio",
    "templates"
  ],
  "author": "AWS CDK Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-samples/aws-cdk-examples.git"
  },
  "bugs": {
    "url": "https://github.com/aws-samples/aws-cdk-examples/issues"
  },
  "homepage": "https://github.com/aws-samples/aws-cdk-examples#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/cdk.out/**"
    ],
    "coverageReporters": [
      "text",
      "html",
      "lcov"
    ]
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "@typescript-eslint/recommended",
      "prettier"
    ],
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-explicit-any": "off"
    },
    "ignorePatterns": [
      "*.js",
      "*.d.ts",
      "node_modules/",
      "cdk.out/"
    ]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2
  }
}