{
  "name": "simple-text-processing-cloudshell-s3",
  "version": "1.0.0",
  "description": "CDK TypeScript application for simple text processing using CloudShell and S3",
  "bin": {
    "simple-text-processing": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "aws-cdk": "2.100.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.100.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21",
    "cdk-nag": "^2.27.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-samples/recipes"
  },
  "keywords": [
    "aws",
    "cdk",
    "typescript",
    "s3",
    "cloudshell",
    "text-processing",
    "infrastructure"
  ],
  "author": "AWS Recipes",
  "license": "MIT-0",
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "lib/**/*.ts",
      "!lib/**/*.d.ts"
    ]
  }
}