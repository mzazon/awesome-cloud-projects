    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLogging": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-efs:denyAnonymousAccess": true,
    "@aws-cdk/aws-opensearchservice:enableLogging": true,
    "@aws-cdk/aws-lambda:codecommitUniqueId": true,
    "@aws-cdk/aws-route53resolver:removeEmptyForwardRuleTargets": true,
    "@aws-cdk/aws-cli:disable-python-deprecation-warnings": true,
    "@aws-cdk/core:suppressStackSizeCheck": false,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk/aws-apigateway:usagePlanKeyOrderInsensitiveId": true,
    "@aws-cdk/core:stackRelativeExports": true,
    "@aws-cdk/aws-rds:lowercaseDbIdentifier": true,
    "@aws-cdk/aws-efs:defaultEncryptionAtRest": true,
    "@aws-cdk/aws-lambda:recognizeVersionPropsString": true,
    "@aws-cdk/aws-apigateway:executeApiArn": true,
    "@aws-cdk/aws-lambda:logRetentionSetting": true,
    "@aws-cdk/aws-kinesisfirehose:deliveryStreamName": true,
    "@aws-cdk/aws-stepfunctions:jsonPathIsPassthrough": true,
    "@aws-cdk/aws-stepfunctions-tasks:tokenPrefix": true,
    "@aws-cdk/aws-ec2:natInstancesUseLatestUserData": true,
    "@aws-cdk/aws-iam:principalPermissionsPolicyNameAndRoleNameInUniqueResourceName": true
  },
  "requireApproval": "never",
  "toolkitStackName": "CDKToolkit",
  "toolkitBucketName": "cdktoolkit-stagingbucket-{random-suffix}",
  "versionReporting": false,
  "pathMetadata": false,
  "assetMetadata": false,
  "notices": true,
  "bundlingStacks": [],
  "lookups": true,
  "hotswap": false,
  "rollback": true,
  "diff": {
    "requireApproval": "broadening",
    "fail": false
  },
  "deploy": {
    "stacks": "*",
    "requireApproval": "never",
    "ci": false,
    "rollback": true,
    "hotswap": false,
    "concurrency": 1,
    "reuseAssets": [],
    "ignoreErrors": false,
    "force": false,
    "logs": true,
    "progress": "events"
  },
  "metadata": {
    "description": "CDK configuration for Chat Notifications with SNS and Chatbot",
    "version": "1.0.0",
    "recipe": "chat-notifications-sns-chatbot",
