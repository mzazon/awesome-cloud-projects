      "repository": "https://github.com/aws-samples/interactive-data-analytics-bedrock",
      "homepage": "https://aws.amazon.com/bedrock/",
      "keywords": [
        "aws",
        "cdk",
        "bedrock",
        "analytics",
        "code-interpreter",
        "serverless",
        "python"
      ]
    },
    "analytics": {
      "enabled": true,
      "version": "v2"
    },
    "cdk": {
      "version": "2.165.0",
      "language": "python",
      "framework": "aws-cdk-lib"
    },
    "aws": {
      "services": [
        "S3",
        "Lambda",
        "API Gateway",
        "CloudWatch",
        "IAM",
        "SQS",
        "Bedrock"
      ],
      "regions": [
        "us-east-1",
        "us-west-2",
        "eu-west-1",
        "ap-southeast-1"
      ]
    }
  },
  "tags": {
    "Project": "InteractiveDataAnalytics",
    "Environment": "Development",
    "Owner": "DataAnalyticsTeam",
    "CostCenter": "Analytics",
    "Compliance": "Enterprise",
    "CDK": "true",
    "Language": "Python",
    "Framework": "AWS-CDK",
    "Service": "Bedrock-Analytics"
  },
  "notifications": {
    "enabled": false
  },
  "bootstrapQualifier": "hnb659fds",
  "@aws-cdk--bootstrap-version": "15"
}