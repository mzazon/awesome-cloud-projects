{
  "app": "npx ts-node --prefer-ts-exts app.ts",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "**/*.d.ts",
      "**/*.js",
      "tsconfig.json",
      "package*.json",
      "yarn.lock",
      "node_modules",
      "test"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:target": "aws-cdk-lib",
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/core:stackRelativeExports": true,
    "@aws-cdk/aws-rds:lowercaseDbIdentifier": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk/aws-logs:duplicateLogGroupsInRegion": "allow",
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-route53:splitHorizonOptIn": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-lambda:codeguruProfilerEnvVarOptOut": true,
    "@aws-cdk/core:prefixLogicalIds": true,
    "@aws-cdk/aws-opensearchservice:enableLogging": true,
    "@aws-cdk/aws-neptune:defaultAuthIamEnabled": true,
    "@aws-cdk/aws-sqs:fifoThroughputLimit": true,
    "@aws-cdk/aws-synthetics:artifactsBucketLocation": true,
    "@aws-cdk/core:enableDiffNoFail": "true"
  },
  "feature_flags": {
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/core:stackRelativeExports": true
  },
  "metadata": {
    "analytics:enabled": false,
    "version:reporting": false
  },
  "build": {
    "outputDir": "cdk.out"
  },
  "deploy": {
    "requireApproval": "broadening"
  },
  "diff": {
    "contextLines": 3
  },
  "notices": false,
  "versionReporting": false,
  "bundlingStacks": [],
  "lookups": false,
  "output": "cdk.out",
  "progress": "bar",
  "pathMetadata": false,
  "assetMetadata": false,
  "profile": null,
  "toolkitStackName": "CDKToolkit",
  "plugin": [],
  "requireApproval": "broadening",
  "debug": false,
  "trace": false,
  "ignoreErrors": false,
  "strict": true,
  "validation": true,
  "concurrency": 1,
  "cachedir": ".cdk.staging",
  "maxConcurrentRuns": 1,
  "tags": {
    "Purpose": "InteractiveDataAnalytics",
    "Environment": "Development",
    "ManagedBy": "CDK"
  }
}