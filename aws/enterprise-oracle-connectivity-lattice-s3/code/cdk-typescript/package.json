{
  "name": "oracle-enterprise-connectivity-cdk",
  "version": "1.0.0",
  "description": "CDK TypeScript application for Oracle Database@AWS Enterprise Connectivity with VPC Lattice and S3",
  "bin": {
    "oracle-enterprise-connectivity": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.25",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "aws-cdk": "2.169.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.169.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "keywords": [
    "aws",
    "cdk",
    "oracle",
    "database",
    "vpc-lattice",
    "s3",
    "redshift",
    "analytics",
    "enterprise"
  ],
  "author": "AWS Solutions Architecture",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-samples/oracle-enterprise-connectivity"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/cdk.out/**"
    ]
  },
  "cdk": {
    "version": "2.169.0",
    "context": {
      "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
      "@aws-cdk/core:checkSecretUsage": true,
      "@aws-cdk/core:target": "aws-cdk-lib",
      "@aws-cdk-lib/aws-autoscaling:generateUserData": true,
      "@aws-cdk-lib/aws-cloudwatch:removeEmptyMetrics": true,
      "@aws-cdk-lib/aws-codebuild:useArtifactPathForApps": true,
      "@aws-cdk-lib/aws-cognito:userPoolExplicitProps": true,
      "@aws-cdk-lib/aws-ec2:enableDnsHostnames": true,
      "@aws-cdk-lib/aws-ecs:disableExplicitDeploymentControllerForCircuitBreaker": true,
      "@aws-cdk-lib/aws-efs:denyAnonymousAccess": true,
      "@aws-cdk-lib/aws-events:eventsTargetQueueSameAccount": true,
      "@aws-cdk-lib/aws-iam:minimizePolicies": true,
      "@aws-cdk-lib/aws-iam:standardizedServicePrincipals": true,
      "@aws-cdk-lib/aws-lambda:recognizeVersionProps": true,
      "@aws-cdk-lib/aws-logs:removeRetentionPolicyResourcePolicy": true,
      "@aws-cdk-lib/aws-rds:lowercaseDbIdentifier": true,
      "@aws-cdk-lib/aws-redshift:columnId": true,
      "@aws-cdk-lib/aws-route53:splitHorizonNs": true,
      "@aws-cdk-lib/aws-route53resolver:bindQueryLog": true,
      "@aws-cdk-lib/aws-s3:createDefaultLoggingPolicy": true,
      "@aws-cdk-lib/aws-sns-subscriptions:restrictSqsDescryption": true,
      "@aws-cdk-lib/aws-stepfunctions:booleanToStrInCondition": true,
      "@aws-cdk-lib/pipelines-1-api-migration-path": true,
      "@aws-cdk-lib/core:includePrefixInUniqueNameGeneration": true,
      "@aws-cdk-lib/aws-opensearchservice:enableOpensearchMultiAzWithStandby": true
    }
  }
}