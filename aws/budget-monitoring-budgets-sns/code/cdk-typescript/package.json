{
  "name": "budget-monitoring-cdk",
  "version": "1.0.0",
  "description": "AWS CDK TypeScript application for comprehensive budget monitoring with AWS Budgets and SNS notifications",
  "bin": {
    "budget-monitoring-cdk": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.24",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "aws-cdk": "2.136.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.136.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21",
    "cdk-nag": "^2.28.91"
  },
  "keywords": [
    "aws",
    "cdk",
    "typescript",
    "budget",
    "monitoring",
    "sns",
    "cost-management",
    "notifications",
    "alerting"
  ],
  "author": "AWS CDK",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-samples/aws-cdk-budget-monitoring"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "lib/**/*.ts",
      "!lib/**/*.d.ts"
    ]
  }
}