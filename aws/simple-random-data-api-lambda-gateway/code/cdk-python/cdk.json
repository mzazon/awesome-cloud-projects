{
  "app": "python3 app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc"
    ]
  },
  "context": {
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "aws-cdk:enableDiffNoFail": true,
    "@aws-cdk/core:stackRelativeExports": true,
    "@aws-cdk/aws-ecr-assets:dockerIgnoreSupport": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLogging": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/core:target-partitions": [
      "aws",
      "aws-cn"
    ],
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-opensearchservice:enableOpensearchMultiAzWithStandby": true,
    "@aws-cdk/aws-lambda-nodejs:useLatestRuntimeVersion": true,
    "@aws-cdk/aws-efs:denyAnonymousAccess": true,
    "@aws-cdk/aws-opensearchservice:enableLogging": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/aws-codeguruprofiler:profilingGroupWithoutComputePlatform": true,
    "@aws-cdk/aws-rds:rdsEngineVersion": true,
    "@aws-cdk/aws-redshift:deferredKeyRevision": true,
    "@aws-cdk/aws-stepfunctions:useCdkId": true,
    "@aws-cdk/aws-kms:reduceCrossAccountPolicyScope": true,
    "@aws-cdk/aws-eks:nodegroupNameAttribute": true,
    "@aws-cdk/aws-ec2:ebsDefaultGp3Volume": true,
    "@aws-cdk/aws-ecs:removeDefaultDeploymentAlarm": true,
    "@aws-cdk/aws-rds:preventRenderingDeprecatedCredentials": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": false,
    "@aws-cdk/aws-codebuild:useBuildEnvironmentAvailableTypesNode": true,
    "@aws-cdk/aws-route53-patterns:useCertificate": true,
    "@aws-cdk/aws-s3:eventBridgeNotificationToSameEnvironment": true,
    "@aws-cdk/aws-codepipeline:crossRegionReplicationBuckets": true,
    "@aws-cdk/aws-s3:defaultEventNotificationMode": "eventbridge",
    "@aws-cdk/aws-cloudwatch-actions:changeLambdaPermissionLogicalIdForLambdaAction": true,
    "@aws-cdk/aws-codepipeline:defaultPipelineTypeToV2": true,
    "@aws-cdk/aws-kms:aliasNameRefWithSuffix": true,
    "@aws-cdk/aws-stepfunctions-tasks:useNewS3UriParametersForLambdaInvoke": true,
    "@aws-cdk/aws-lambda-python:usePoetryInstallOption": true,
    "@aws-cdk/aws-appsync:useArnForSourceApiAssociationIdentifier": true,
    "@aws-cdk/aws-rds:auroraClusterChangeScopeOfInstanceParameterGroupWithEachParameters": true,
    "@aws-cdk/aws-appsync:useArnForSourceApiAssociationIdentifier": true,
    "@aws-cdk/aws-stepfunctions:includeStateMachineNameInDefinitionHash": true,
    "@aws-cdk/aws-lambda:defineRuntime": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-rds:setCorrectValueForDatabaseSecret": true,
    "@aws-cdk/aws-lambda:automaticAsyncInvocation": true,
    "@aws-cdk/aws-stepfunctions:stateMachineNameIsNotTruncated": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ec2:disableLookupDifferentPlatforms": true,
    "@aws-cdk/aws-logs:logRetentionUseArnForLogRetentionResource": true,
    "@aws-cdk/aws-stepfunctions:useCdkId": true,
    "@aws-cdk/pipelines-cli-cdk:useCli2": true,
    "@aws-cdk/aws-applicationautoscaling:generateConfigurationOnProps": true,
    "@aws-cdk/aws-codedeploy:removeAlarmsFromDeploymentGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/aws-events:connectCrossStackTargets": true,
    "@aws-cdk/aws-rds:databaseProxyUniqueResourceName": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-lambda:useLatestRuntimeVersion": true
  }
}