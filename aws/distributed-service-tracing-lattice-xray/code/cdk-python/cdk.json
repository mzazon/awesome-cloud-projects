  "build": {
    "commands": [
      "python -m pip install -r requirements.txt",
      "python -m pytest tests/ -v --cov=. --cov-report=html",
      "python -m black --check .",
      "python -m flake8 .",
      "python -m mypy .",
      "python -m bandit -r . -f json",
      "python -m safety check"
    ]
  },
  "diff": {
    "context": 3,
    "template": true,
    "changeset": true,
    "securityChanges": true,
    "fail": false
  },
  "deploy": {
    "requireApproval": "broadening",
    "rollback": true,
    "hotswap": true,
    "watch": true,
    "logs": true,
    "concurrency": 1,
    "ci": false
  },
  "destroy": {
    "force": false,
    "exclusively": false
  },
  "metadata": {
    "analytics": false,
    "notices": true,
    "version": "2.162.1",
    "versionReporting": false
