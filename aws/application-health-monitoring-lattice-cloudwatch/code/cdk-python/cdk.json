{
  "app": "python app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc",
      ".venv/**",
      ".env/**",
      "venv/**",
      "env/**",
      ".pytest_cache/**",
      ".mypy_cache/**",
      "**/.DS_Store",
      "**/node_modules/**",
      "cdk.out/**",
      ".git/**",
      "**/*.log"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk/aws-apigateway:usagePlanKeyOrderInsensitiveId": true,
    "@aws-cdk/core:stackRelativeExports": true,
    "@aws-cdk/aws-rds:lowercaseDbIdentifier": true,
    "@aws-cdk/aws-lambda:recognizeUnstableNodejsRuntime": true,
    "@aws-cdk/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/core:target-partitions": [
      "aws",
      "aws-cn"
    ],
    "@aws-cdk/aws-lambda-nodejs:useLatestRuntimeVersion": true,
    "@aws-cdk/aws-ec2:defaultSubnetLocationContextKey": "AWS::EC2::Subnet::AvailabilityZone",
    "@aws-cdk/aws-lambda:deprecatedRuntimeBehavior": "error",
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-route53-patters:useCertificate": true,
    "@aws-cdk/customresources:installLatestAwsSdkDefault": false,
    "@aws-cdk/aws-rds:databaseProxyUniqueResourceName": true,
    "@aws-cdk/aws-codedeploy:removeAlarmsFromDeploymentGroup": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLoggingProperty": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-opensearchservice:enableLoggingProperty": true,
    "@aws-cdk/aws-lambda-nodejs:alwaysBundleModules": true,
    "@aws-cdk/aws-lambda:recognizeUnstablePythonRuntime": true,
    "@aws-cdk/aws-apigatewayv2:enablePartitionBasedGlobalResourceName": true
  },
  "toolkit": {
    "version": 2,
    "rollback": false
  },
  "build": {
    "exclude": [
      "cdk.out",
      ".venv",
      ".env",
      "venv",
      "env",
      "__pycache__",
      "*.pyc",
      ".pytest_cache",
      ".mypy_cache",
      ".DS_Store",
      "node_modules",
      ".git",
      "*.log"
    ]
  },
  "diff": {
    "include": [
      "app.py",
      "requirements.txt",
      "setup.py"
    ]
  },
  "deploy": {
    "requireApproval": "never",
    "rollback": true,
    "confirmChangeset": false,
    "ci": false,
    "progress": "events",
    "execute": true,
    "changeSet": false,
    "hotswap": false,
    "concurrency": 1
  },
  "doctor": {
    "enabled": true
  },
  "notices": true,
  "versionReporting": true,
  "pathMetadata": true,
  "assetMetadata": true,
  "staging": true,
  "output": "cdk.out",
  "profile": null,
  "plugin": [],
  "requireApproval": "never",
  "tags": {
    "Project": "VPC-Lattice-Health-Monitoring",
    "Environment": "production",
    "ManagedBy": "AWS-CDK",
    "Purpose": "health-monitoring",
    "CostCenter": "infrastructure",
    "Owner": "platform-team"
  },
  "bootstrapKmsKeyId": null,
  "lookups": true,
  "bundlingStacks": [],
  "debug": false,
  "force": false,
  "json": false,
  "strict": true,
  "trace": false,
  "verbose": false,
  "ignoreErrors": false,
  "ec2creds": false,
  "cachedir": null,
  "roleArn": null,
  "proxy": null,
  "caBundlePath": null,
  "config": {
    "deployment": {
      "account": "${CDK_DEFAULT_ACCOUNT}",
      "region": "${CDK_DEFAULT_REGION}"
    },
    "monitoring": {
      "enabled": true,
      "retain_logs": 30,
      "detailed_monitoring": true
    },
    "security": {
      "enable_cdk_nag": true,
      "encryption_at_rest": true,
      "encryption_in_transit": true,
      "least_privilege_iam": true
    },
    "cost_optimization": {
      "use_spot_instances": false,
      "lifecycle_policies": true,
      "reserved_capacity": false
    },
    "vpc_lattice": {
      "auth_type": "AWS_IAM",
      "health_check": {
        "interval_seconds": 30,
        "timeout_seconds": 5,
        "healthy_threshold": 2,
        "unhealthy_threshold": 2,
        "path": "/health"
      }
    },
    "cloudwatch": {
      "alarms": {
        "error_rate_threshold": 10,
        "timeout_threshold": 5,
        "response_time_threshold": 2000,
        "evaluation_periods": 2
      },
      "dashboard": {
        "enabled": true,
        "auto_refresh": true
      }
    },
    "lambda": {
      "runtime": "python3.12",
      "timeout_minutes": 5,
      "memory_mb": 256,
      "log_retention_days": 30,
      "environment_variables": {
        "LOG_LEVEL": "INFO"
      }
    },
    "sns": {
      "delivery_retry_policy": true,
      "dead_letter_queue": false
    }
  },
  "feature_flags": {
    "@aws-cdk/core:newStyleStackSynthesis": true,
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "@aws-cdk/aws-secretsmanager:parseOwnedSecretName": true,
    "@aws-cdk/aws-kms:defaultKeyPolicies": true,
    "@aws-cdk/aws-s3:grantWriteWithoutAcl": true,
    "@aws-cdk/aws-ecs-patterns:removeDefaultDesiredCount": true,
    "@aws-cdk/aws-rds:lowercaseDbIdentifier": true,
    "@aws-cdk/aws-efs:defaultEncryptionAtRest": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true
  },
  "parameters": {
    "vpc_id": {
      "description": "VPC ID to use for VPC Lattice service network (leave empty to use default VPC)",
      "type": "String",
      "default": "",
      "allowedPattern": "^(vpc-[a-f0-9]{8,17})?$"
    },
    "email_endpoints": {
      "description": "Comma-separated list of email addresses for SNS notifications",
      "type": "CommaDelimitedList",
      "default": ""
    },
    "environment_name": {
      "description": "Environment name for resource tagging and naming",
      "type": "String",
      "default": "production",
      "allowedValues": ["development", "staging", "production"]
    },
    "cost_allocation_tags": {
      "description": "Additional cost allocation tags in JSON format",
      "type": "String",
      "default": "{}"
    }
  },
  "metadata": {
    "version": "1.0.0",
    "description": "CDK application for VPC Lattice health monitoring with CloudWatch and auto-remediation",
    "author": "AWS CDK Generator",
    "created": "2025-01-16",
    "aws_services": [
      "VPC Lattice",
      "CloudWatch",
      "Lambda",
      "SNS",
      "EC2",
      "IAM",
      "CloudWatch Logs"
    ],
    "architecture_patterns": [
      "Health Monitoring",
      "Auto-Remediation",
      "Observability",
      "Event-Driven Architecture"
    ],
    "compliance": [
      "AWS Well-Architected Framework",
      "Security Best Practices",
      "Cost Optimization",
      "Operational Excellence"
    ]
  }
}