{
  "app": "python app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc",
      ".venv/**",
      ".env",
      ".git/**",
      ".gitignore",
      "**/.DS_Store",
      "**/node_modules/**",
      "**/*.log"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk/aws-rds:lowercaseDbIdentifier": true,
    "@aws-cdk/aws-rds:generatePhysicalName": true,
    "@aws-cdk/aws-apigateway:usagePlanKeyOrderInsensitiveId": true,
    "@aws-cdk/aws-rds:auroraClusterChangeSizeToScalingConfig": true,
    "@aws-cdk/aws-route53-resolver:removeEmptyForwardRuleAndTargetIps": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": false,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-route53-resolver:populateBindTargetId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableEmrServicePolicyV2": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:useNewS3UriParametersForLambdaInvoke": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/aws-lambda:addLambdaFunctionUrlAuthType": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-efs:denyAnonymousAccess": true,
    "@aws-cdk/aws-opensearchservice:enableOpensearchMultiAzWithStandby": true,
    "@aws-cdk/aws-lambda-nodejs:useLatestRuntimeVersion": true,
    "@aws-cdk/aws-efs:mountTargetOrderInsensitiveLogicalId": true,
    "@aws-cdk/aws-rds:auroraClusterChangeSizeToServerlessV2": true,
    "@aws-cdk/aws-stepfunctions-tasks:sfnTasksDisableUpdateWorkflowApiLogging": false,
    "@aws-cdk/aws-route53-resolver:enableUpdatePropsValidation": true,
    "@aws-cdk/aws-lambda:defineRuntime": true,
    "@aws-cdk/aws-s3:bucketNotificationsHandlerRoleArn": true,
    "@aws-cdk/aws-stepfunctions-tasks:useS3ExpressUriParametersForLambdaInvoke": true,
    "@aws-cdk/aws-lambda:useCurrentVersionForLambdaDigest": true
  },
  "toolkitStackName": "CDKToolkit",
  "toolkitBucket": {
    "versioned": true
  },
  "requireApproval": "never",
  "lookups": false,
  "bundlingStacks": [],
  "assetMetadata": false,
  "pathMetadata": false,
  "versionReporting": false,
  "staging": true,
  "output": "cdk.out",
  "notices": true,
  "rollback": true,
  "hotswap": false,
  "hotswapFallbackEnabled": false,
  "profile": "",
  "plugin": [],
  "debug": false,
  "trace": false,
  "strict": false,
  "validation": true,
  "verbose": false,
  "tags": {
    "Project": "api-integration-agentcore-gateway",
    "Environment": "development",
    "Owner": "DevOps Team",
    "CostCenter": "Engineering",
    "Application": "Enterprise API Integration",
    "Version": "1.0.0"
  },
  "bootstrapKmsKeyId": "",
  "advancedOptions": {
    "maxResources": 1000,
    "minifyJson": true,
    "compressAssets": true
  },
  "featureFlags": {
    "@aws-cdk/core:newStyleStackSynthesis": true,
    "@aws-cdk/core:stackRelativeExports": true,
    "@aws-cdk/aws-ecr-assets:dockerIgnoreSupport": true,
    "@aws-cdk/aws-secretsmanager:parseOwnedSecretName": true,
    "@aws-cdk/aws-kms:defaultKeyPolicies": true,
    "@aws-cdk/aws-s3:grantWriteWithoutAcl": true,
    "@aws-cdk/aws-ecs-patterns:removeDefaultDesiredCount": true,
    "@aws-cdk/aws-rds:lowercaseDbIdentifier": true,
    "@aws-cdk/aws-efs:defaultEncryptionAtRest": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true
  }
}