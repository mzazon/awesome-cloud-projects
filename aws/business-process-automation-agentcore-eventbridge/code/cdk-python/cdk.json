{
  "app": "python3 app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:target-partitions": [
      "aws",
      "aws-cn"
    ],
    "@aws-cdk-containers/ecs-service-extensions:enableDefaultLogDriver": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ecs:disableExplicitDeploymentControllerForCircuitBreaker": true,
    "@aws-cdk/aws-iam:importedRoleStackSafeDefaultPolicyName": true,
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-route53-patters:useCertificate": true,
    "@aws-cdk/customresources:installLatestAwsSdkDefault": false,
    "@aws-cdk/aws-rds:databaseProxyUniqueResourceName": true,
    "@aws-cdk/aws-codedeploy:removeAlarmsFromDeploymentGroup": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLoggingConfiguration": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-efs:denyAnonymousAccess": true,
    "@aws-cdk/aws-opensearchservice:enableLogging": true,
    "@aws-cdk/aws-lambda:useLatestRuntimeVersion": true,
    "@aws-cdk/aws-ecs:removeDefaultDeploymentAlarm": false,
    "@aws-cdk/core:bootstrapQualifier": "hnb659fds",
    "@aws-cdk/core:stackRelativeExports": true,
    "@aws-cdk/aws-rds:preventRenderingDeprecatedCredentials": true,
    "@aws-cdk/aws-apigateway:logGroupCreateBeforeFirstDeployment": true,
    "@aws-cdk/aws-ec2:ebsDefaultGp3Volume": true,
    "@aws-cdk/aws-ecs-patterns:removeDefaultDesiredCount": true,
    "@aws-cdk/aws-rds:setCorrectValueForDatabaseInstanceReadReplicaInstanceResourceId": true,
    "@aws-cdk/aws-route53-resolver:FirewallRuleGroupEvaluationOrder": true,
    "@aws-cdk/core:cfnIncludeRejectComplexResourceUpdateCreatePolicy": false,
    "@aws-cdk/aws-lambda-nodejs:sdkV3ExcludeSmithyPackages": true,
    "@aws-cdk/aws-stepfunctions:useCfnIncludeRole": false,
    "@aws-cdk/aws-lambda-nodejs:autoIdFromFileName": true
  },
  "build": "python -m py_compile app.py",
  "output": "cdk.out",
  "requireApproval": "never",
  "lookups": true,
  "metadata": {
    "aws:cdk:version": "2.150.0",
    "aws:cdk:application-type": "business-process-automation",
    "aws:cdk:project-type": "serverless-ai",
    "aws:cdk:language": "python",
    "aws:cdk:recipe-name": "business-process-automation-agentcore-eventbridge",
    "aws:cdk:recipe-version": "1.1",
    "aws:cdk:description": "Intelligent business process automation with Bedrock Agents and EventBridge",
    "aws:cdk:services": [
      "bedrock",
      "eventbridge", 
      "lambda",
      "s3",
      "iam",
      "cloudwatch"
    ],
    "aws:cdk:architecture-patterns": [
      "event-driven",
      "serverless",
      "ai-automation",
      "document-processing"
    ]
  },
  "feature-flags": {
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "@aws-cdk/core:enableDiffNoFail": true,
    "aws-cdk:enableDiffNoFail": true
  },
  "toolkit-stack-name": "CDKToolkit",
  "bootstrap": {
    "bucket-name": "cdk-bootstrap-bucket",
    "qualifier": "hnb659fds",
    "trust": [
      "arn:aws:iam::*:root"
    ],
    "cloudformation-execution-policies": [
      "arn:aws:iam::aws:policy/AdministratorAccess"
    ]
  },
  "staging": {
    "enabled": true
  },
  "path-metadata": true,
  "asset-metadata": true,
  "version-reporting": true,
  "notices": true,
  "bundling": {
    "stacks": []
  },
  "hotswap": false,
  "profile": "default",
  "region": "us-east-1",
  "rollback": true,
  "tags": {
    "Project": "BusinessProcessAutomation",
    "Environment": "Development",
    "CostCenter": "Engineering",
    "Owner": "DevOps-Team",
    "Application": "Document-Processing-AI",
    "Recipe": "business-process-automation-agentcore-eventbridge"
  },
  "plugin": [],
  "parameters": {},
  "outputs": "cdk-outputs.json",
  "ci": false,
  "debug": false,
  "trace": false,
  "strict": true,
  "validation": true,
  "json": false,
  "yaml": false,
  "no-execute": false,
  "no-staging": false,
  "force": false,
  "exclusively": false,
  "concurrency": 1,
  "asset-parallelism": true,
  "asset-prebuild": true,
  "progress": "bar"
}