{
  "name": "standardized-service-deployment-lattice-catalog",
  "version": "1.0.0",
  "description": "CDK TypeScript application for standardized VPC Lattice service deployment with Service Catalog",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy --all",
    "deploy:templates": "cdk deploy VpcLatticeTemplates",
    "deploy:catalog": "cdk deploy ServiceCatalog",
    "deploy:main": "cdk deploy StandardizedServiceDeployment",
    "destroy": "cdk destroy --all",
    "diff": "cdk diff",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.100.0",
    "cdk-nag": "^2.27.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "keywords": [
    "aws",
    "cdk",
    "vpc-lattice",
    "service-catalog",
    "infrastructure",
    "standardization",
    "service-mesh",
    "governance"
  ],
  "author": "Platform Engineering Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/standardized-service-deployment"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}