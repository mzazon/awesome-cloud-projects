{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "baseName": {
      "value": "mywebsite"
    },
    "location": {
      "value": "eastus"
    },
    "storageAccountReplication": {
      "value": "Standard_LRS"
    },
    "storageAccessTier": {
      "value": "Hot"
    },
    "cdnSku": {
      "value": "Standard_Microsoft"
    },
    "enableCompression": {
      "value": true
    },
    "queryStringCachingBehavior": {
      "value": "IgnoreQueryString"
    },
    "indexDocument": {
      "value": "index.html"
    },
    "errorDocument": {
      "value": "404.html"
    },
    "environment": {
      "value": "dev"
    },
    "purpose": {
      "value": "static-website-hosting"
    },
    "owner": {
      "value": "Web Development Team"
    }
  }
}