{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "metadata": {
    "description": "Production parameters for Azure Maps Store Locator deployment with enhanced security and performance optimizations"
  },
  "parameters": {
    "mapsAccountName": {
      "value": "mapstore-prod-001"
    },
    "location": {
      "value": "eastus"
    },
    "environment": {
      "value": "prod"
    },
    "projectName": {
      "value": "store-locator"
    },
    "costCenter": {
      "value": "retail-operations"
    },
    "enableSystemIdentity": {
      "value": true
    },
    "disableLocalAuth": {
      "value": false
    },
    "additionalDataProcessingRegions": {
      "value": [
        "westus2",
        "northeurope"
      ]
    },
    "corsRules": {
      "value": [
        {
          "allowedOrigins": [
            "https://yourdomain.com",
            "https://www.yourdomain.com",
            "https://stores.yourdomain.com"
          ]
        }
      ]
    }
  }
}