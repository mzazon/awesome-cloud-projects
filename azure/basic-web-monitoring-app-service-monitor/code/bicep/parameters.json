{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "my-webapp-demo"
    },
    "appServicePlanName": {
      "value": "my-webapp-plan"
    },
    "logAnalyticsWorkspaceName": {
      "value": "my-logs-workspace"
    },
    "location": {
      "value": "East US"
    },
    "appServicePlanSku": {
      "value": "F1"
    },
