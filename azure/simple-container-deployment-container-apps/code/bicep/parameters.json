{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "eastus"
    },
    "baseName": {
      "value": "containerapp-demo"
    },
    "environment": {
      "value": "dev"
    },
    "containerImage": {
      "value": "mcr.microsoft.com/k8se/quickstart:latest"
    },
    "containerPort": {
      "value": 80
    },
    "enableExternalIngress": {
      "value": true
    },
    "minReplicas": {
      "value": 0
    },
    "maxReplicas": {
      "value": 5
    },
    "cpuAllocation": {
      "value": "0.25"
    },
    "memoryAllocation": {
      "value": "0.5Gi"
    },
    "tags": {
      "value": {
        "environment": "dev",
        "purpose": "recipe-demo",
        "managed-by": "bicep",
        "project": "azure-recipes",
        "cost-center": "engineering"
      }
    }
  }
}