{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "East US"
    },
    "environment": {
      "value": "dev"
    },
    "applicationName": {
      "value": "configdemo"
    },
    "uniqueSuffix": {
      "value": "[uniqueString(resourceGroup().id)]"
    },
    "appServicePlanSku": {
      "value": "F1"
    },
    "appConfigSku": {
      "value": "free"
    },
    "configurationValues": {
      "value": {
        "DemoApp:Settings:Title": "Configuration Management Demo",
        "DemoApp:Settings:BackgroundColor": "#2563eb",
        "DemoApp:Settings:Message": "Hello from Azure App Configuration!",
        "DemoApp:Settings:RefreshInterval": "30",
        "DemoApp:Database:ConnectionTimeout": "30",
        "DemoApp:Logging:LogLevel": "Information",
        "DemoApp:Features:EnableCache": "true",
        "DemoApp:Api:MaxRetryAttempts": "3"
      }
    },
    "tags": {
      "value": {
        "environment": "dev",
        "application": "configdemo",
        "purpose": "recipe-demo",
        "owner": "platform-team",
        "cost-center": "engineering",
        "project": "azure-recipes"
      }
    }
  }
}