{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "baseName": {
      "value": "simpleweb"
    },
    "location": {
      "value": "eastus"
    },
    "acrSku": {
      "value": "Basic"
    },
    "enableAdminUser": {
      "value": true
    },
    "containerImageName": {
      "value": "simple-nginx"
    },
    "containerImageTag": {
      "value": "v1"
    },
    "containerCpuCores": {
      "value": 1
    },
    "containerMemoryInGb": {
      "value": 1
    },
    "containerPort": {
      "value": 80
    },
    "restartPolicy": {
      "value": "Always"
    },
    "tags": {
      "value": {
        "purpose": "recipe",
        "environment": "demo",
        "recipe": "simple-web-container-aci-registry",
        "costCenter": "development",
        "owner": "platform-team"
      }
    }
  }
}